local function testSizes()
	local ScrollingFrame = Instance.new("ScrollingFrame")
	local MenuFrame = Instance.new("Frame")
	MenuFrame.Name = "test"
	MenuFrame.Visible = true
	MenuFrame.Parent = LDSHUBGlobalTestes.Frame
	MenuFrame.BackgroundColor3 = Color3.fromRGB(52, 52, 52)
	MenuFrame.Position = UDim2.new(0.273959696, 0, 0.0941176489, 0)
	MenuFrame.Size = UDim2.new(0.707664907, 0, 0.871586502, 0)
	
	ScrollingFrame.Parent = MenuFrame
	ScrollingFrame.Active = true
	ScrollingFrame.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	ScrollingFrame.BackgroundTransparency = 1.000
	ScrollingFrame.BorderColor3 = Color3.fromRGB(0, 0, 0)
	ScrollingFrame.BorderSizePixel = 0
	ScrollingFrame.CanvasSize = UDim2.new(0,0,2,0)
	ScrollingFrame.Position = UDim2.new(0.025253389, 0, 0.0944860876, 0)
	ScrollingFrame.Size = UDim2.new(0.966852427, 0, 0.877616346, 0)
	
	local bt,_ = createClickButton("test","",ScrollingFrame, "test")
	defaultOffset = ToOffset(bt.Parent)
	bt.Parent.Size = UDim2.new(0.839, 0,0.058, 0)
	defaultOffsetpequeno = ToOffset(bt.Parent)
	MenuFrame:Destroy()
end

-- Menu Utilities



local function offMenus()
	for _,v in pairs(LDSHUBGlobalTestes.Frame:GetChildren()) do
		if v:IsA("Frame") and v.Name ~= "Warn" and v.Name ~= "LE" then
			v.Visible = false
		end
	end
	for _,v in pairs(LDSHUBGlobalTestes.GButtons:GetChildren()) do
		if v:IsA("TextButton") then
			v.BackgroundColor3 = Color3.fromRGB(126, 126, 126)
		end
	end
end

local NameSave = game.Players.LocalPlayer.Name.."LDS"

local function LoadSettings()
	local HttpService = game:service('HttpService')

	--use 'isfile' to check if we are reading the correct txt file

	if isfile(NameSave) then
		--use 'readfile' to read 'TUTORIAL.txt' and decode it
		G.Settings = HttpService:JSONDecode(readfile(NameSave))
	end
end

local function SaveSettings()
	local JSON -- is nil
	local HttpService = game:service('HttpService') -- used for the JSON DECODE AND ENCODE
	--Decode
	JSON = HttpService:JSONEncode(G.Settings)
	--Make a text file using 'writefile'
	writefile(NameSave,JSON)
	-- write a code: check if the file was saved: yes - return true - no - return false. 
	if isfile(NameSave) then
		return true
	else
		return false
	end
end

-- Warns

function module:ConfirmWarn(callback)
	LDSHUBGlobalTestes.Warn.Visible = true
	LDSHUBGlobalTestes.Dwarn.Visible = true
	LDSHUBGlobalTestes.Yes.MouseButton1Click:Connect(function()
		LDSHUBGlobalTestes.Warn.Visible = false
		LDSHUBGlobalTestes.Dwarn.Visible = false
		callback()
	end)
	LDSHUBGlobalTestes.No.MouseButton1Click:Connect(function()
		LDSHUBGlobalTestes.Warn.Visible = false
		LDSHUBGlobalTestes.Dwarn.Visible = false
		return false
	end)
end

function module:OkWarn(text)
	LDSHUBGlobalTestes.Warn.Visible = true
	LDSHUBGlobalTestes.Swarn.Visible = true
	
	LDSHUBGlobalTestes.TextLabel_13.Text = text
	LDSHUBGlobalTestes.Okay_2.MouseButton1Click:Connect(function()
		LDSHUBGlobalTestes.Warn.Visible = false
		LDSHUBGlobalTestes.Swarn.Visible = false
	end)
end

function module:DevWarn()
	LDSHUBGlobalTestes.Warn.Visible = true
	LDSHUBGlobalTestes.Wwarn.Visible = true
	LDSHUBGlobalTestes.Okay.MouseButton1Click:Connect(function()
		LDSHUBGlobalTestes.Warn.Visible = false
		LDSHUBGlobalTestes.Wwarn.Visible = false
	end)
end

function module:activatedWarn()
	LDSHUBGlobalTestes.Warn.Visible = true
	LDSHUBGlobalTestes.Swarn.Visible = true
	LDSHUBGlobalTestes.TextLabel_13.Text = "Activate the desired mode!"
	LDSHUBGlobalTestes.Okay_2.MouseButton1Click:Connect(function()
		LDSHUBGlobalTestes.Warn.Visible = false
		LDSHUBGlobalTestes.Swarn.Visible = false
	end)
end

function module:bannedWarn()
	LDSHUBGlobalTestes.Warn.Visible = true
	LDSHUBGlobalTestes.Bwarn.Visible = true
	LDSHUBGlobalTestes.Okay_3.MouseButton1Click:Connect(function()
		LDSHUBGlobalTestes.Warn.Visible = false
		LDSHUBGlobalTestes.Bwarn.Visible = false
	end)
end

function module:SendNotification(msg,buttonName, enabled)
	local notifFrame = createNotBt(msg,buttonName, enabled)
	task.spawn(function()
		task.wait(3)
		notifFrame:Destroy()
	end)
end

function module:showFastTP() -- TP USO LIBRARY
	LDSHUBGlobalTestes.Teleports.Visible = true
end

function module:hideFastTP() -- TP USO LIBRARY
	LDSHUBGlobalTestes.Teleports.Visible = false
end

function activeMobileUi()
	LDSHUBGlobalTestes._1Home.Size = UDim2.new(0.854, 0,0.047, 0)
	LDSHUBGlobalTestes._998Settings.Size = UDim2.new(0.854, 0,0.047, 0)
	LDSHUBGlobalTestes._999UpdateNotes.Size = UDim2.new(0.854, 0,0.047, 0)
	defaultButtonSize = UDim2.new(0.854, 0,0.047, 0)
	LDSHUBGlobalTestes.LE.Size = UDim2.new(0.362, 0,1, 0)
	LDSHUBGlobalTestes.LE.Position = UDim2.new(-0.124, 0,0, 0)
	LDSHUBGlobalTestes.Frame.Size = UDim2.new(0.495, 0,0.52, 0)
end

local UIS = game:GetService("UserInputService")

function module:start(gameName, Hversion, isPremium)
	local awsframe
	if UIS.TouchEnabled then
		activeMobileUi()
	end
	if isPremium then
		LDSHUBGlobalTestes.UIGradient1.Enabled = true
		LDSHUBGlobalTestes.UIGradient2.Enabled = true
		LDSHUBGlobalTestes.UIGradient3.Enabled = true
	else
		LDSHUBGlobalTestes.UIGradient1.Enabled = false
		LDSHUBGlobalTestes.UIGradient2.Enabled = false
		LDSHUBGlobalTestes.UIGradient3.Enabled = false
	end
	--if game.GameId == 4582358979 then
	--	local awsframe = createAWSSettings()
	--end
	LDSHUBGlobalTestes.Frame.Draggable = true
	LDSHUBGlobalTestes.Frame.Selectable = true
	LDSHUBGlobalTestes.Frame.Active = true
	LDSHUBGlobalTestes.GameName.Text = gameName
	LDSHUBGlobalTestes.HubVersion.Text = "v. " .. Hversion
	LDSHUBGlobalTestes.UpdatesVTXT.Text = "v. " .. Hversion
	LDSHUBGlobalTestes.LDSHUBGlobalTestes.Enabled = true
	
	ajustarLat(LDSHUBGlobalTestes._1Home)
	ajustarLat(LDSHUBGlobalTestes._998Settings)
	ajustarLat(LDSHUBGlobalTestes._999UpdateNotes)
	testSizes()
	
	local creditosss = createCredits()
	task.spawn(function()
		task.wait(2)
		creditosss:Destroy()
	end)
	local frame = LDSHUBGlobalTestes.LDSHUBGlobalTestes.Frame
	frame.HomeUi.Visible = true
	
	LDSHUBGlobalTestes.ButtonLeft.MouseButton1Click:Connect(function() -- TP PAGES
		if tpPageNow > 1 then
			tpPageNow = tpPageNow - 1
			renderPage(tpPageNow)
		end
	end)
	
	LDSHUBGlobalTestes.ButtonRight.MouseButton1Click:Connect(function() -- TP PAGES
		if tpPageNow < maxPages then
			tpPageNow = tpPageNow + 1
			renderPage(tpPageNow)
		end
	end)

	-- Funes principais da gui global
	LDSHUBGlobalTestes._1Home.MouseButton1Click:Connect(function()
		offMenus()
		LDSHUBGlobalTestes._1Home.BackgroundColor3 = Color3.fromRGB(170, 0, 255)
		frame.HomeUi.Visible = true
	end)
	
	LDSHUBGlobalTestes._998Settings.MouseButton1Click:Connect(function()
		offMenus()
		LDSHUBGlobalTestes._998Settings.BackgroundColor3 = Color3.fromRGB(170, 0, 255)
		--if awsframe then
		--	awsframe.Visible = true
		--else
		frame.SettingsUI.Visible = true
		--end
	end)
	LDSHUBGlobalTestes._999UpdateNotes.MouseButton1Click:Connect(function()
		offMenus()
		LDSHUBGlobalTestes._999UpdateNotes.BackgroundColor3 = Color3.fromRGB(170, 0, 255)
		frame.UpdateNoteUi.Visible = true
	end)

	-- Crditos botes
	LDSHUBGlobalTestes.TextButtonYT.MouseButton1Click:Connect(function()
		setclipboard("https://www.youtube.com/@Sr_LDS")
		game.StarterGui:SetCore("SendNotification", {
			Title = "Youtube",
			Text = "Copied! Paste in your browser!",
			Duration = 6,
		})
	end)
	LDSHUBGlobalTestes.TextButtonDC.MouseButton1Click:Connect(function()
		setclipboard("https://discord.gg/sr-lds-community-17500-1034615067402055720")
		game.StarterGui:SetCore("SendNotification", {
			Title = "Discord",
			Text = "Copied! Paste in your browser!",
			Duration = 6,
		})
	end)

	-- Settings botes
	LoadSettings()
	LDSHUBGlobalTestes.AntAFKButton.MouseButton1Click:Connect(function()
		afkActive = not afkActive
		local vu = game:GetService("VirtualUser")
		local uis = game:GetService("UserInputService")
		local Pl = game.Players.LocalPlayer
		
		spawn(function()
			Pl.Idled:Connect(function()
				vu:Button2Down(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
				wait(1)
				vu:Button2Up(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
			end)
		end)
		
		local function clickAndShift()
			vu:CaptureController()
			vu:ClickButton2(Vector2.new())
			uis.InputBegan:Fire({ KeyCode = Enum.KeyCode.LeftShift })
			uis.InputEnded:Fire({ KeyCode = Enum.KeyCode.LeftShift })
		end
		task.spawn(function()
			while afkActive do
				clickAndShift()
				task.wait(300) -- 5 minutos
			end
		end)
		
	end)

	LDSHUBGlobalTestes.SaveButton.MouseButton1Click:Connect(function()
		if SaveSettings() == true then
			game.StarterGui:SetCore("SendNotification", {
				Title = "LDS HUB",
				Text = "Settings successfully saved!",
				Duration = 6,
			})
		else
			game.StarterGui:SetCore("SendNotification", {
				Title = "LDS HUB",
				Text = "Error when saving settings!",
				Duration = 6,
			})
		end
	end)

	LDSHUBGlobalTestes.ResetSaveButton.MouseButton1Click:Connect(function()
		G.Settings = {}
		if SaveSettings() == true then
			game.StarterGui:SetCore("SendNotification", {
				Title = "LDS HUB",
				Text = "Settings successfully reset!",
				Duration = 6,
			})
		else
			game.StarterGui:SetCore("SendNotification", {
				Title = "LDS HUB",
				Text = "Error when reseting settings!",
				Duration = 6,
			})
		end
	end)

	LDSHUBGlobalTestes.DeleteButton.MouseButton1Click:Connect(function()
		local function dstroy()
			for i, v in pairs(G.Settings) do
				G.Settings[i] = false
			end
			LDSHUBGlobalTestes.LDSHUBGlobalTestes:Destroy()
			script:Destroy()
		end
		module:ConfirmWarn(dstroy)
	end)

	-- Hide Button
	LDSHUBGlobalTestes.HideButton.MouseButton1Click:Connect(function()
		LDSHUBGlobalTestes.Frame.Visible = not LDSHUBGlobalTestes.Frame.Visible
	end)

	-- Creating menu options
	local window = {}

	function window:addTab(name)
		menusCount = menusCount + 1
		local cloneFrame = createMenuFrame(name)

		local cloneBt = createMenuButton(name)
		cloneBt.MouseButton1Click:Connect(function()
			offMenus()
			cloneBt.BackgroundColor3 = Color3.fromRGB(170, 0, 255)
			if defaultOffsetLateral then
				cloneBt.Size = defaultOffsetLateral
			else
				cloneBt.Size = defaultButtonSize
			end
			cloneFrame.Visible = true
		end)

		local tab = {}

		tab.MenuFrame = cloneFrame
		tab.OptionFrame = cloneFrame.Options

		function tab:addToggle(text, description, size, premium, callback)
			local togBt, modBt, helpIcon = createToggleButton(text, description, tab.MenuFrame.ScrollingFrame, size)

			local value
			if typeof(premium) == "boolean" and premium then
				createPremiumBg(togBt.Parent)
				helpIcon.ImageColor3 = Color3.fromRGB(52, 52, 52)
				togBt.Parent.BackgroundColor3 = Color3.fromRGB(186, 107, 255)
			end

			local function changeVisual() 
				if value == true then
					task.spawn(function()
						modBt.BackgroundColor3 = Color3.fromRGB(0, 255, 0)
						modBt:TweenPosition(UDim2.new(0.5, 0, 0.064, 0))
						modBt.Position = UDim2.new(0.5, 0, 0.064, 0)
					end)
				else
					task.spawn(function()
						modBt.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
						modBt:TweenPosition(UDim2.new(0.057, 0, 0.064, 0))
						modBt.Position = UDim2.new(0.057, 0, 0.064, 0)
					end)
				end
			end

			if table.find(G.Settings, text) then
				value = G.Settings[text]
				changeVisual()
			else
				value = false
			end

			if description ~= '' then
				helpIcon.MouseEnter:Connect(function()
					cloneFrame.HelpFrame.HelpText.Text = description
					cloneFrame.HelpFrame.Visible = true
				end)
				helpIcon.MouseLeave:Connect(function()
					cloneFrame.HelpFrame.Visible = false
				end)
			end
			togBt.MouseButton1Click:Connect(function()
				local execute = true
				if typeof(premium) == "boolean" and premium and not isPremium then
					execute = false
				end
				if execute then
					value = not value
					G.Settings[text] = value
					changeVisual()
					if typeof(premium) == "function" then
						premium(value)
					elseif typeof(premium) == "boolean" then
						callback(value)
					end
				else
					module:OkWarn("This is a premium feature!")
				end
			end)
			local toggle = {}
			
			function toggle:Disable()
				value = false
				G.Settings[text] = value
				changeVisual()
			end
			return toggle
		end

		function tab:addClick(text, description, size, premium, callback)
			local clickBt, helpIcon = createClickButton(text, description, tab.MenuFrame.ScrollingFrame,size)

			if typeof(premium) == "boolean" and premium then
				createPremiumBg(clickBt.Parent)
				helpIcon.ImageColor3 = Color3.fromRGB(52, 52, 52)
				clickBt.Parent.BackgroundColor3 = Color3.fromRGB(186, 107, 255)
			end

			if description ~= '' then
				helpIcon.MouseEnter:Connect(function()
					cloneFrame.HelpFrame.HelpText.Text = description
					cloneFrame.HelpFrame.Visible = true
				end)
				helpIcon.MouseLeave:Connect(function()
					cloneFrame.HelpFrame.Visible = false
				end)
			end
			clickBt.MouseButton1Click:Connect(function()
				local execute = true

				if execute then
					if typeof(premium) == "function" then
						premium()
					elseif typeof(premium) == "boolean" then
						callback()
					end
				end
			end)	
		end

		function tab:addCombo(text, description, hasList, optsFull)
			local selectBt, helpIcon = createComboButton(text, description, tab.MenuFrame.ScrollingFrame)
			local value

			if table.find(G.Settings, text) then
				value = G.Settings[text]
				selectBt.Text = value
			else
				value = ""
			end
			if description ~= '' then
				helpIcon.MouseEnter:Connect(function()
					cloneFrame.HelpFrame.HelpText.Text = description
					cloneFrame.HelpFrame.Visible = true
				end)
				helpIcon.MouseLeave:Connect(function()
					cloneFrame.HelpFrame.Visible = false
				end)
			end
			
			local opts
			
			if hasList then
				local helpFrameSize = cloneFrame.HelpFrame.Size
				selectBt.MouseEnter:Connect(function()
					
				end)
			else
				opts = optsFull
			end
			
			selectBt.MouseButton1Click:Connect(function()
				local execute = true

				tab.MenuFrame.ScrollingFrame.Visible = false
				local scroll = tab.OptionFrame.ScrollableOpt
				local baseOption = scroll.Option
				tab.OptionFrame.ComboTitle.Text = text

				-- Funes da gui

				local searchBox = tab.OptionFrame.SearchBox
				local searchF
				searchF = searchBox:GetPropertyChangedSignal("Text"):Connect(function()
					for _, v in pairs(scroll:GetChildren()) do
						if v.Name == "NewOption" then
							v:Destroy()
						end
					end

					local search = searchBox.Text:lower()
					for i, v in pairs(opts) do
						if string.sub(v:lower(), 1, #search) == search then
							local newOpt = baseOption:Clone()
							newOpt.Name = "NewOption"
							newOpt.Text = v
							newOpt.Parent = scroll
							newOpt.Visible = true

							newOpt.MouseButton1Click:Connect(function()
								value = v
								G.Settings[text] = v
								selectBt.Text = tostring(v)
								tab.OptionFrame.Visible = false
								tab.MenuFrame.ScrollingFrame.Visible = true
								for _, v in pairs(scroll:GetChildren()) do
									if v.Name == "NewOption" then
										v:Destroy()
									end
								end
								searchBox.Text = ""
								searchF:Disconnect()
							end)
						end
					end
				end)

				local closeF
				closeF = tab.OptionFrame.Close.MouseButton1Click:Connect(function()
					tab.OptionFrame.Visible = false
					for _, v in pairs(scroll:GetChildren()) do
						if v.Name == "NewOption" then
							v:Destroy()
						end
					end
					tab.MenuFrame.ScrollingFrame.Visible = true
					searchBox.Text = ""
					searchF:Disconnect()
					closeF:Disconnect()
				end)



				--  Funes / Opes
				for _, v in pairs(opts) do
					local clone = baseOption:Clone()
					clone.Parent = scroll
					clone.Visible = true
					clone.Name = "NewOption"
					clone.Text = v
					ajustarScrollCombo(clone)
					clone.MouseButton1Click:Connect(function()
						value = v
						selectBt.Text = tostring(v)
						tab.OptionFrame.Visible = false
						tab.MenuFrame.ScrollingFrame.Visible = true
						for _, v in pairs(scroll:GetChildren()) do
							if v.Name == "NewOption" then
								v:Destroy()
							end
						end
						searchBox.Text = ""
						searchF:Disconnect()
					end)
				end
				updateCanvasSize(scroll, scroll.UIGridLayout)
				tab.OptionFrame.Visible = true
			end)
			local combo = {}
			function combo:getValue()
				local toReturn
				if value ~= "" then
					toReturn = value
				end
				return toReturn
			end
			return combo
		end

		function tab:addSlider(text, description, min, max, callback)
			local mouse = game.Players.LocalPlayer:GetMouse()
			local movingSlider = false
			local SliderClick, DescIcon, sliderBG, SliderValue = createSlider(text, description, tab.MenuFrame.ScrollingFrame)
			local value = min

			local snapAmount = 1
			local pixelsFromEdge = 0


			if description ~= '' then
				DescIcon.MouseEnter:Connect(function()
					cloneFrame.HelpFrame.HelpText.Text = description
					cloneFrame.HelpFrame.Visible = true
				end)
				DescIcon.MouseLeave:Connect(function()
					cloneFrame.HelpFrame.Visible = false
				end)
			end
			-- muda a pos da agulha
			SliderClick.MouseButton1Down:Connect(function()
				movingSlider = true
			end)
			SliderClick.MouseButton1Up:Connect(function()
				movingSlider = false
			end)
			UIS.InputEnded:Connect(function(input, gameProcessedEvent)
				if not gameProcessedEvent and input.UserInputType == Enum.UserInputType.MouseButton1 then
					movingSlider = false
				end
			end)

			mouse.Move:Connect(function()
				if movingSlider then

					local xOffset = math.floor((mouse.X - sliderBG.AbsolutePosition.X) / snapAmount + 0.5) * snapAmount
					local xOffsetClamped = math.clamp(xOffset, pixelsFromEdge, sliderBG.AbsoluteSize.X - pixelsFromEdge)

					local sliderPosNew = UDim2.new(0, xOffsetClamped, 0, SliderClick.Position.Y)

					SliderClick.Position = sliderPosNew

					local roundedAbsSize = math.floor(sliderBG.AbsoluteSize.X / snapAmount + 0.5) * snapAmount
					local roundedOffsetClamped = math.floor(xOffsetClamped / snapAmount + 0.5) * snapAmount

					local sliderValue = roundedOffsetClamped / roundedAbsSize

					value = math.round(sliderValue * max)
					if value < min then
						value = min
					end
					SliderValue.Text = value
					if callback ~= nil then
						callback(value)
					end
				end
			end)

			local sliderReturn = {}

			function sliderReturn:getValue()
				return value
			end

			return sliderReturn
		end

		function tab:addInputBox(text)
			local TextBox = createInputBox(text, tab.MenuFrame.ScrollingFrame)
			local textboxM = {}
			function textboxM:getValue()
				return TextBox.Text
			end
			return textboxM
		end

		function tab:addMultiCombo(text,description, opts)
			local selectBt, helpIcon = createComboButton(text, description, tab.MenuFrame.ScrollingFrame)
			local value = {}
			
			if description ~= '' then
				helpIcon.MouseEnter:Connect(function()
					cloneFrame.HelpFrame.HelpText.Text = description
					cloneFrame.HelpFrame.Visible = true
				end)
				helpIcon.MouseLeave:Connect(function()
					cloneFrame.HelpFrame.Visible = false
				end)
			end
			
			selectBt.MouseButton1Click:Connect(function()
				local execute = true

				tab.MenuFrame.ScrollingFrame.Visible = false
				local scroll = tab.OptionFrame.ScrollableOpt
				local baseOption = scroll.Option
				tab.OptionFrame.ComboTitle.Text = text.." (Multi Selection)"

				local allSel = false
				if #value == #opts then
					allSel = true
				end

				-- Funes da gui

				local searchBox = tab.OptionFrame.SearchBox
				local searchF
				searchF = searchBox:GetPropertyChangedSignal("Text"):Connect(function()
					for _, v in pairs(scroll:GetChildren()) do
						if v.Name == "NewOption" then
							v:Destroy()
						end
					end




					local search = searchBox.Text:lower()
					for i, v in pairs(opts) do
						if string.sub(v:lower(), 1, #search) == search then
							local newOpt = baseOption:Clone()
							newOpt.Name = "NewOption"
							newOpt.Text = v
							newOpt.Parent = scroll
							newOpt.Visible = true
							local stroke = Instance.new("UIStroke")
							stroke.Parent = newOpt
							stroke.Enabled = false
							stroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
							stroke.Thickness = 3
							stroke.Color = Color3.fromRGB(56, 255, 21)
							if not allSel and table.find(value, v) then
								stroke.Enabled = true
							end
							newOpt.MouseButton1Click:Connect(function()
								if stroke.Enabled == false then
									stroke.Enabled = true
									table.insert(value, newOpt.Text)
								else
									stroke.Enabled = false
									table.remove(value, table.find(value, newOpt.Text))
									if scroll.AllOptions.UIStroke.Enabled then
										scroll.AllOptions.UIStroke.Enabled = false
									end
								end
								if #value > 0 then
									selectBt.Text = "..."
								end
							end)
						end
					end
					
					
				end)

				local closeF
				closeF = tab.OptionFrame.Close.MouseButton1Click:Connect(function()
					tab.OptionFrame.Visible = false
					for _, v in pairs(scroll:GetChildren()) do
						if v.Name == "NewOption" or v.Name == "AllOptions" then
							v:Destroy()
						end
					end
					tab.MenuFrame.ScrollingFrame.Visible = true
					searchBox.Text = ""
					searchF:Disconnect()
					closeF:Disconnect()
				end)

				

				--  Funes / Opes
				for _, v in pairs(opts) do
					local clone = baseOption:Clone()
					clone.Parent = scroll
					clone.Visible = true
					clone.Name = "NewOption"
					clone.Text = v
					ajustarScrollCombo(clone)
					local stroke = Instance.new("UIStroke")
					stroke.Parent = clone
					stroke.Enabled = false
					stroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
					stroke.Thickness = 3
					stroke.Color = Color3.fromRGB(56, 255, 21)
					if not allSel and table.find(value, v) then
						stroke.Enabled = true
					end
					clone.MouseButton1Click:Connect(function()
						if stroke.Enabled == false then
							
							if scroll.AllOptions.UIStroke.Enabled then
								scroll.AllOptions.UIStroke.Enabled = false
								value = {v}
								stroke.Enabled = true
							else
								stroke.Enabled = true
								table.insert(value, clone.Text)
							end
							
						else
							stroke.Enabled = false
							table.remove(value, table.find(value, v))

						end
						if #value > 0 then
							selectBt.Text = "..."
						end
					end)
				end
				local allOpt = baseOption:Clone()
				allOpt.Name = "AllOptions"
				allOpt.LayoutOrder = -10
				allOpt.Text = "All Options"
				allOpt.Parent = scroll
				allOpt.Visible = true
				ajustarScrollCombo(allOpt)
				local strokeAll = Instance.new("UIStroke")
				strokeAll.Parent = allOpt
				strokeAll.Enabled = false
				strokeAll.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
				strokeAll.Thickness = 3
				strokeAll.Color = Color3.fromRGB(56, 255, 21)
				if allSel then
					strokeAll.Enabled = true
				end
				
				
				allOpt.MouseButton1Click:Connect(function()
					if strokeAll.Enabled then
						strokeAll.Enabled = false
					else
						strokeAll.Enabled = true
					end
					for _,v in pairs(scroll:GetChildren()) do
						if v:IsA("TextButton") and v.Name ~= "Option" and v ~= allOpt then
							if strokeAll.Enabled then
								v.UIStroke.Enabled = false
								value = opts
							else
								v.UIStroke.Enabled = false
								value = {}
							end
						end
					end
				end)
				tab.OptionFrame.Visible = true
			end)
			local combo = {}
			function combo:getValue()
				local toReturn
				if #value > 0 then
					toReturn = value
				end
				return toReturn
			end
			return combo
		end

		function tab:addFolder(text)
			local folderBt = createFolderButton(text, tab.MenuFrame)

			-- Funo / Criao do scrollFrame
			local cloneScroll = tab.MenuFrame.ScrollingFrame:Clone()
			cloneScroll.Parent = tab.MenuFrame
			cloneScroll.Visible = false
			cloneScroll.Name = "folderScroll"
			-- Limpar scroll novo
			for _,v in pairs(cloneScroll:GetChildren()) do
				if not v:IsA("UIListLayout") then
					v:Destroy()
				end
			end

			-- Funo / Boto
			folderBt.MouseButton1Click:Connect(function()
				local menuName = tab.MenuFrame.MenuTitle.Text
				tab.MenuFrame.MenuTitle.Text = text
				tab.MenuFrame.ScrollingFrame.Visible = false
				cloneScroll.Visible = true
				tab.MenuFrame.BackFolder.Visible = true
				tab.MenuFrame.BackFolder.MouseButton1Click:Connect(function()
					tab.MenuFrame.ScrollingFrame.Visible = true
					cloneScroll.Visible = false
					tab.MenuFrame.BackFolder.Visible = false
					tab.MenuFrame.MenuTitle.Text = menuName
				end)
			end)
			-- Funes / Folder
			local folder = {}

			function folder:addToggle(text, description, size, premium, callback)
				local togBt, modBt, helpIcon = createToggleButton(text, description, cloneScroll,size)
				local value

				if typeof(premium) == "boolean" and premium then
					createPremiumBg(togBt.Parent)
					helpIcon.ImageColor3 = Color3.fromRGB(52, 52, 52)
					togBt.Parent.BackgroundColor3 = Color3.fromRGB(186, 107, 255)
				end

				local function changeVisual() 
					if value == true then
						task.spawn(function()
							modBt.BackgroundColor3 = Color3.fromRGB(0, 255, 0)
							modBt:TweenPosition(UDim2.new(0.5, 0, 0.064, 0))
						end)
					else
						task.spawn(function()
							modBt.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
							modBt:TweenPosition(UDim2.new(0.057, 0, 0.064, 0))
						end)
					end
				end

				if table.find(G.Settings, text) then
					value = G.Settings[text]
					changeVisual()
				else
					value = false
				end

				if description ~= '' then
					helpIcon.MouseEnter:Connect(function()
						cloneFrame.HelpFrame.HelpText.Text = description
						cloneFrame.HelpFrame.Visible = true
					end)
					helpIcon.MouseLeave:Connect(function()
						cloneFrame.HelpFrame.Visible = false
					end)
				end
				togBt.MouseButton1Click:Connect(function()
					local execute = true
					if typeof(premium) == "boolean" and premium and not isPremium then
						execute = false
					end
					if execute then
						value = not value
						G.Settings[text] = value
						changeVisual()
						if typeof(premium) == "function" then
							premium(value)
						elseif typeof(premium) == "boolean" then
							callback(value)
						end
					else
						module:OkWarn("This is a premium feature!")
					end
				end)
				local toggle = {}

				function toggle:Disable()
					value = false
					G.Settings[text] = value
					changeVisual()
				end
				return toggle
			end

			function folder:addClick(text, description, size, premium, callback)
				local clickBt, helpIcon = createClickButton(text, description, cloneScroll,size)
				
				if typeof(premium) == "boolean" and premium then
					createPremiumBg(clickBt.Parent)
					helpIcon.ImageColor3 = Color3.fromRGB(52, 52, 52)
					clickBt.Parent.BackgroundColor3 = Color3.fromRGB(186, 107, 255)
				end

				if description ~= '' then
					helpIcon.MouseEnter:Connect(function()
						cloneFrame.HelpFrame.HelpText.Text = description
						cloneFrame.HelpFrame.Visible = true
					end)
					helpIcon.MouseLeave:Connect(function()
						cloneFrame.HelpFrame.Visible = false
					end)
				end
				clickBt.MouseButton1Click:Connect(function()
					local execute = true

					if execute then
						if typeof(premium) == "function" then
							premium()
						elseif typeof(premium) == "boolean" then
							callback()
						end
					end
				end)	
			end

			function folder:addCombo(text, description, opts)
				local selectBt, helpIcon = createComboButton(text, description, cloneScroll)
				local value

				if table.find(G.Settings, text) then
					value = G.Settings[text]
					selectBt.Text = value
				else
					value = ""
				end
				if description ~= '' then
					helpIcon.MouseEnter:Connect(function()
						cloneFrame.HelpFrame.HelpText.Text = description
						cloneFrame.HelpFrame.Visible = true
					end)
					helpIcon.MouseLeave:Connect(function()
						cloneFrame.HelpFrame.Visible = false
					end)
				end
				selectBt.MouseButton1Click:Connect(function()
					local execute = true

					tab.MenuFrame.ScrollingFrame.Visible = false
					local scroll = tab.OptionFrame.ScrollableOpt
					local baseOption = scroll.Option
					tab.OptionFrame.ComboTitle.Text = text

					-- Funes da gui

					local searchBox = tab.OptionFrame.SearchBox
					local searchF
					searchF = searchBox:GetPropertyChangedSignal("Text"):Connect(function()
						for _, v in pairs(scroll:GetChildren()) do
							if v.Name == "NewOption" then
								v:Destroy()
							end
						end

						local search = searchBox.Text:lower()
						for i, v in pairs(opts) do
							if string.sub(v:lower(), 1, #search) == search then
								local newOpt = baseOption:Clone()
								newOpt.Name = "NewOption"
								newOpt.Text = v
								newOpt.Parent = scroll
								newOpt.Visible = true

								newOpt.MouseButton1Click:Connect(function()
									value = v
									G.Settings[text] = v
									selectBt.Text = tostring(v)
									tab.OptionFrame.Visible = false
									tab.MenuFrame.ScrollingFrame.Visible = true
									for _, v in pairs(scroll:GetChildren()) do
										if v.Name == "NewOption" then
											v:Destroy()
										end
									end
									searchBox.Text = ""
									searchF:Disconnect()
								end)
							end
						end
					end)

					local closeF
					closeF = tab.OptionFrame.Close.MouseButton1Click:Connect(function()
						tab.OptionFrame.Visible = false
						for _, v in pairs(scroll:GetChildren()) do
							if v.Name == "NewOption" then
								v:Destroy()
							end
						end
						cloneScroll.Visible = true
						searchBox.Text = ""
						searchF:Disconnect()
						closeF:Disconnect()
					end)



					--  Funes / Opes
					for _, v in pairs(opts) do
						local clone = baseOption:Clone()
						clone.Parent = scroll
						clone.Visible = true
						clone.Name = "NewOption"
						clone.Text = v
						ajustarScrollCombo(clone)
						clone.MouseButton1Click:Connect(function()
							value = v
							selectBt.Text = tostring(v)
							tab.OptionFrame.Visible = false
							cloneScroll.Visible = true
							for _, v in pairs(scroll:GetChildren()) do
								if v.Name == "NewOption" then
									v:Destroy()
								end
							end
							searchBox.Text = ""
							searchF:Disconnect()
						end)
					end
					updateCanvasSize(scroll, scroll.UIGridLayout)
					tab.OptionFrame.Visible = true
				end)
				local combo = {}
				function combo:getValue()
					local toReturn
					if value ~= "" then
						toReturn = value
					end
					return toReturn
				end
				return combo
			end

			function folder:addSlider(text, description, min, max, callback)
				local mouse = game.Players.LocalPlayer:GetMouse()
				local movingSlider = false
				local SliderClick, DescIcon, sliderBG, SliderValue = createSlider(text, description, cloneScroll)
				updateCanvasSize(cloneScroll, cloneScroll.UIListLayout)
				local value = min

				if description ~= '' then
					DescIcon.MouseEnter:Connect(function()
						cloneFrame.HelpFrame.HelpText.Text = description
						cloneFrame.HelpFrame.Visible = true
					end)
					DescIcon.MouseLeave:Connect(function()
						cloneFrame.HelpFrame.Visible = false
					end)
				end
				-- muda a pos da agulha
				SliderClick.MouseButton1Down:Connect(function()
					movingSlider = true
				end)
				LDSHUBGlobalTestes.Frame.MouseButton1Up:Connect(function()
					movingSlider = false
				end)
				UIS.InputEnded:Connect(function(input, gameProcessedEvent)
					if not gameProcessedEvent and input.UserInputType == Enum.UserInputType.MouseButton1 then
						movingSlider = false
					end
				end)

				local snapAmount = 1
				local pixelsFromEdge = 0

				mouse.Move:Connect(function()
					if movingSlider then

						local xOffset = math.floor((mouse.X - sliderBG.AbsolutePosition.X) / snapAmount + 0.5) * snapAmount
						local xOffsetClamped = math.clamp(xOffset, pixelsFromEdge, sliderBG.AbsoluteSize.X - pixelsFromEdge)

						local sliderPosNew = UDim2.new(0, xOffsetClamped, 0, SliderClick.Position.Y)

						SliderClick.Position = sliderPosNew

						local roundedAbsSize = math.floor(sliderBG.AbsoluteSize.X / snapAmount + 0.5) * snapAmount
						local roundedOffsetClamped = math.floor(xOffsetClamped / snapAmount + 0.5) * snapAmount

						local sliderValue = roundedOffsetClamped / roundedAbsSize

						value = math.round(sliderValue * max)
						if value < min then
							value = min
						end
						SliderValue.Text = value
						if callback ~= nil then
							callback(value)
						end
					end
				end)

				local sliderReturn = {}

				function sliderReturn:getValue()
					return value
				end

				return sliderReturn
			end

			function folder:addInputBox(text)
				local TextBox = createInputBox(text, cloneScroll)
				local textboxM = {}
				function textboxM:getValue()
					return TextBox.Text
				end
				return textboxM
			end

			function folder:addMultiCombo(text,description, opts)
				local selectBt, helpIcon = createComboButton(text, description, cloneScroll)
				local value = {}

				if description ~= '' then
					helpIcon.MouseEnter:Connect(function()
						cloneFrame.HelpFrame.HelpText.Text = description
						cloneFrame.HelpFrame.Visible = true
					end)
					helpIcon.MouseLeave:Connect(function()
						cloneFrame.HelpFrame.Visible = false
					end)
				end

				selectBt.MouseButton1Click:Connect(function()
					local execute = true

					tab.MenuFrame.ScrollingFrame.Visible = false
					local scroll = tab.OptionFrame.ScrollableOpt
					local baseOption = scroll.Option
					tab.OptionFrame.ComboTitle.Text = text.." (Multi Selection)"

					local allSel = false
					if #value == #opts then
						allSel = true
					end

					-- Funes da gui

					local searchBox = tab.OptionFrame.SearchBox
					local searchF
					searchF = searchBox:GetPropertyChangedSignal("Text"):Connect(function()
						for _, v in pairs(scroll:GetChildren()) do
							if v.Name == "NewOption" then
								v:Destroy()
							end
						end




						local search = searchBox.Text:lower()
						for i, v in pairs(opts) do
							if string.sub(v:lower(), 1, #search) == search then
								local newOpt = baseOption:Clone()
								newOpt.Name = "NewOption"
								newOpt.Text = v
								newOpt.Parent = scroll
								newOpt.Visible = true
								local stroke = Instance.new("UIStroke")
								stroke.Parent = newOpt
								stroke.Enabled = false
								stroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
								stroke.Thickness = 3
								stroke.Color = Color3.fromRGB(56, 255, 21)
								if not allSel and table.find(value, v) then
									stroke.Enabled = true
								end
								newOpt.MouseButton1Click:Connect(function()
									if stroke.Enabled == false then
										stroke.Enabled = true
										table.insert(value, newOpt.Text)
									else
										stroke.Enabled = false
										table.remove(value, table.find(value, newOpt.Text))
										if scroll.AllOptions.UIStroke.Enabled then
											scroll.AllOptions.UIStroke.Enabled = false
										end
									end
									if #value > 0 then
										selectBt.Text = "..."
									end
								end)
							end
						end


					end)

					local closeF
					closeF = tab.OptionFrame.Close.MouseButton1Click:Connect(function()
						tab.OptionFrame.Visible = false
						for _, v in pairs(scroll:GetChildren()) do
							if v.Name == "NewOption" or v.Name == "AllOptions" then
								v:Destroy()
							end
						end
						cloneScroll.Visible = true
						searchBox.Text = ""
						searchF:Disconnect()
						closeF:Disconnect()
					end)



					--  Funes / Opes
					for _, v in pairs(opts) do
						local clone = baseOption:Clone()
						clone.Parent = scroll
						clone.Visible = true
						clone.Name = "NewOption"
						clone.Text = v
						ajustarScrollCombo(clone)
						local stroke = Instance.new("UIStroke")
						stroke.Parent = clone
						stroke.Enabled = false
						stroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
						stroke.Thickness = 3
						stroke.Color = Color3.fromRGB(56, 255, 21)
						if not allSel and table.find(value, v) then
							stroke.Enabled = true
						end
						clone.MouseButton1Click:Connect(function()
							if stroke.Enabled == false then

								if scroll.AllOptions.UIStroke.Enabled then
									scroll.AllOptions.UIStroke.Enabled = false
									value = {v}
									stroke.Enabled = true
								else
									stroke.Enabled = true
									table.insert(value, clone.Text)
								end

							else
								stroke.Enabled = false
								table.remove(value, table.find(value, v))

							end
							if #value > 0 then
								selectBt.Text = "..."
							end
						end)
					end
					local allOpt = baseOption:Clone()
					allOpt.Name = "AllOptions"
					allOpt.LayoutOrder = -10
					allOpt.Text = "All Options"
					allOpt.Parent = scroll
					allOpt.Visible = true
					ajustarScrollCombo(allOpt)
					local strokeAll = Instance.new("UIStroke")
					strokeAll.Parent = allOpt
					strokeAll.Enabled = false
					strokeAll.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
					strokeAll.Thickness = 3
					strokeAll.Color = Color3.fromRGB(56, 255, 21)
					if allSel then
						strokeAll.Enabled = true
					end


					allOpt.MouseButton1Click:Connect(function()
						if strokeAll.Enabled then
							strokeAll.Enabled = false
						else
							strokeAll.Enabled = true
						end
						for _,v in pairs(scroll:GetChildren()) do
							if v:IsA("TextButton") and v.Name ~= "Option" and v ~= allOpt then
								if strokeAll.Enabled then
									v.UIStroke.Enabled = false
									value = opts
								else
									v.UIStroke.Enabled = false
									value = {}
								end
							end
						end
					end)
					tab.OptionFrame.Visible = true
				end)
				local combo = {}
				function combo:getValue()
					local toReturn
					if #value > 0 then
						toReturn = value
					end
					return toReturn
				end
				return combo
			end
			
			function folder:addFolder(text)
				
			end

			return folder
		end

		return tab
	end

	return window
end

