local function createPremiumBg(parent)
	local ImageLabel = Instance.new("ImageLabel")
	local UIStroke = Instance.new("UIStroke")
	local UICorner = Instance.new("UICorner")

	--Properties:

	ImageLabel.Parent = parent
	ImageLabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	ImageLabel.BackgroundTransparency = 1.000
	ImageLabel.BorderColor3 = Color3.fromRGB(0, 0, 0)
	ImageLabel.BorderSizePixel = 0
	ImageLabel.Position = UDim2.new(1.19955956e-07, 0, 5.61482693e-07, 0)
	ImageLabel.Size = UDim2.new(0.998, 0,7.785, 0)
	ImageLabel.ZIndex = 0
	ImageLabel.Image = "http://www.roblox.com/asset/?id=14762333689"
	ImageLabel.ImageTransparency = 0.510

	UIStroke.Parent = parent
	UIStroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Contextual
	UIStroke.LineJoinMode = Enum.LineJoinMode.Round
	UIStroke.Thickness = 1
	UIStroke.Color = Color3.fromRGB(255,255,255)
	parent.ClipsDescendants = true
	parent.Parent.BackgroundColor3 = Color3.fromRGB(186, 107, 255)

	UICorner.Parent = ImageLabel

	-- Scripts:

	local function ENAIZ_fake_script() -- ImageLabel.LocalScript 
		local img = ImageLabel
		local TweenService = game:GetService("TweenService")

		TweenService:Create(img, TweenInfo.new(5, Enum.EasingStyle.Linear, Enum.EasingDirection.InOut, -1), {Position = UDim2.new(0, 0, -1, 0)}):Play()
	end
	coroutine.wrap(ENAIZ_fake_script)()

end

