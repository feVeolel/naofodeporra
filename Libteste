	ButtonTitle.TextSize = 14.000
	ButtonTitle.TextWrapped = true
	ButtonTitle.TextXAlignment = Enum.TextXAlignment.Left

	DescIcon.Name = "DescIcon"
	DescIcon.Visible = false
	DescIcon.Parent = Button
	DescIcon.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	DescIcon.BackgroundTransparency = 1.000
	DescIcon.BorderColor3 = Color3.fromRGB(0, 0, 0)
	DescIcon.BorderSizePixel = 0
	DescIcon.Position = UDim2.new(6.4683814, 0, 0.0551959947, 0)
	DescIcon.Size = UDim2.new(0.485887796, 0, 0.883135915, 0)
	DescIcon.Image = "rbxassetid://14592412395"
	if desc ~= "" then
		DescIcon.Visible = true
	end

	ImageLabel.Parent = Button
	ImageLabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	ImageLabel.BackgroundTransparency = 1.000
	ImageLabel.BorderColor3 = Color3.fromRGB(0, 0, 0)
	ImageLabel.BorderSizePixel = 0
	ImageLabel.Position = UDim2.new(0.229611576, 0, -0.0551959947, 0)
	ImageLabel.Size = UDim2.new(0.602, 0,0.994, 0)
	ImageLabel.Image = "rbxassetid://14673874142"

	if defaultOffset ~= nil then
		if size == "small" then
			ClickBt.Size = defaultOffsetpequeno
		else
			ClickBt.Size = defaultOffset
		end
	end
	
	if size ~= "test" then
		ajustarScroll(ClickBt)
	end

	UICorner_2.Parent = ClickBt
	return Button, DescIcon
end

