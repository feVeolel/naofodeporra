	G.PLK_LDSHUB_Game = G.GerarSenha("Gw49xnQdXrP0waCktki04bUR0jocgB3g")
else
	G.PLK_LDSHUB_Game = G.GerarSenha("fais u elli!")
	module = {"You are dumb amigo!"}
end

