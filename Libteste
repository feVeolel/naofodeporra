--UIListLayout.HorizontalAlignment = Enum.HorizontalAlignment.Center
--UIListLayout.HorizontalFlex = Enum.UIFlexMode.Fill
--UIListLayout.VerticalAlignment = Enum.VerticalAlignment.Top
--UIListLayout.VerticalFlex = Enum.UIFlexMode.Fill
--GButtons.CanvasSize = UDim2.new(0,0,0.4,0)

-- Menu Creation

function ToOffset(ui)
	local AB_Size = ui.Parent.AbsoluteSize
	local LB_Size = ui.AbsoluteSize
	--Logo.Size = UDim2.new(0, AB_Size.X * LB_Size.X, 0, AB_Size.Y * LB_Size.Y) -- Previous Method which uses math then sets the UDim2
	ui.Size = UDim2.new(0, LB_Size.X, 0, LB_Size.Y) -- newer method which just reads a value and then sets the UDim2
	return UDim2.new(0, LB_Size.X, 0, LB_Size.Y)
end

function AdjustPaddingToOffset(uiListLayout)
	local parentSize = uiListLayout.Parent.AbsoluteSize
	local padding = uiListLayout.Padding

	-- Converting scale to offset
	local paddingOffset = UDim.new(0, parentSize.X * padding.Scale + padding.Offset)

	uiListLayout.Padding = paddingOffset
end



local defaultOffset
local defaultOffsetpequeno
local defaultOffsetCombo
local defaultOffsetLateral

function ajustarScroll(ui)
	ui.parent.CanvasSize = UDim2.new(0,0,(#ui.Parent:GetChildren()-1)*0.1625,0)
end

function ajustarLat(ui)
	if defaultOffsetLateral == nil then
		ToOffset(ui)
		defaultOffsetLateral = ui.Size
	else
		ui.Size = defaultOffsetLateral
	end
	AdjustPaddingToOffset(ui.Parent.UIListLayout)
	ui.parent.CanvasSize = UDim2.new(0,0,(#ui.Parent:GetChildren()-1)*0.153,0)
end

function ajustarScrollCombo(ui)
	print(#ui.Parent:GetChildren())
	if #ui.Parent:GetChildren() == 3 then
		ToOffset(ui)
		defaultOffsetCombo = ui.Size
	else
		ui.Size = defaultOffsetCombo
	end
	ui.parent.CanvasSize = UDim2.new(0,0,math.floor((#ui.Parent:GetChildren()-1)/3)*0.105,0)
end

local defaultButtonSize = UDim2.new(0.943, 0,0.07, 0)

local function updateCanvasSize(Canvas, Constraint)
	return true
end



