local notGreenColor = Color3.fromRGB(0, 255, 127)
local notRedColor = Color3.fromRGB(255, 0, 127)

local tpPageNow = 1
local maxPages = 1
local pages = {}

local function renderPage(page)
	for _,v in pairs(LDSHUBGlobalTestes.FrameButtons:GetChildren()) do
		if v:IsA("ImageButton") then
			v.Visible = false
		end
	end
	for _,v in pairs(pages[page]) do
		v.Visible = true
	end
end

local function addBtFastTP(Image, name)
	local TeleportButtonTp = Instance.new("ImageButton")
	local UICornerTp = Instance.new("UICorner")
	UICornerTp.CornerRadius = UDim.new(0.21074909, 0)
	TeleportButtonTp.Name = "TeleportButton"
	TeleportButtonTp.Parent = LDSHUBGlobalTestes.FrameButtons
	TeleportButtonTp.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	TeleportButtonTp.BorderColor3 = Color3.fromRGB(0, 0, 0)
	TeleportButtonTp.BorderSizePixel = 0
	TeleportButtonTp.Position = UDim2.new(0.126903549, 0, 0, 0)
	TeleportButtonTp.Size = UDim2.new(0.0964467004, 0, 1, 0)
	TeleportButtonTp.Image = Image
	TeleportButtonTp.Name = name

	if #pages == 0 then
		pages[1] = {TeleportButtonTp}
	else
		if #pages[maxPages] < 9 then
			table.insert(pages[maxPages], TeleportButtonTp)
		else
			maxPages = maxPages + 1
			pages[maxPages] = {TeleportButtonTp}
		end
	end

	UICornerTp.Parent = TeleportButtonTp
	return TeleportButtonTp
end

function module:addTPButton(name, image, pos)
	local tpBt = addBtFastTP(image, name)
	tpBt.MouseEnter:Connect(function()
		LDSHUBGlobalTestes.TextLabel_19.Text = name
	end)
	tpBt.MouseLeave:Connect(function()
		LDSHUBGlobalTestes.TextLabel_19.Text = "LDS HUB!"
	end)

	tpBt.MouseButton1Click:Connect(function()
		game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = pos
	end)
	renderPage(tpPageNow)
end

function module:removeTPButton(name)
	local bt = LDSHUBGlobalTestes.FrameButtons:FindFirstChild(name)
	if bt then
		-- Encontra a pgina e ndice do boto
		local pageIndex, buttonIndex
		for i, page in ipairs(pages) do
			for j, button in ipairs(page) do
				if button.Name == name then
					pageIndex = i
					buttonIndex = j
					break
				end
			end
			if pageIndex then break end
		end

		-- Remove o boto da pgina e do FrameButtons
		table.remove(pages[pageIndex], buttonIndex)
		bt:Destroy()

		-- Reorganiza os botes para preencher as pginas anteriores
		for i = pageIndex, maxPages do
			if #pages[i] == 0 and i < maxPages then
				table.remove(pages, i)
				maxPages = maxPages - 1
				i = i - 1
			elseif #pages[i] < 9 and pages[i+1] then
				table.insert(pages[i], table.remove(pages[i+1], 1))
				if #pages[i+1] == 0 then
					table.remove(pages, i+1)
					maxPages = maxPages - 1
				end
			end
		end

		-- Ajusta a pgina atual se necessrio
		if tpPageNow > maxPages then
			tpPageNow = maxPages
		end

		renderPage(tpPageNow)
	end
end


local function createNotBt(msg, btName, enabled)
	local FrameNot = Instance.new("Frame")
	local UICornerNot = Instance.new("UICorner")
	local UIGradientNot = Instance.new("UIGradient")
	local TextLabelNot = Instance.new("TextLabel")
	local TextLabel_2Not = Instance.new("TextLabel")
	local NameButtonNot = Instance.new("TextLabel")

	FrameNot.Parent = LDSHUBGlobalTestes.Notify
	FrameNot.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	FrameNot.BorderColor3 = Color3.fromRGB(0, 0, 0)
	FrameNot.BorderSizePixel = 0
	FrameNot.Position = UDim2.new(0.0441176482, 0, 0.673684239, 0)
	FrameNot.Size = UDim2.new(0.911764681, 0, 0.200000003, 0)
	
	local uistroke = Instance.new("UIStroke")
	uistroke.Color = Color3.new(1, 1, 1)
	uistroke.Thickness = 2.0999999046325684
	uistroke.Parent = FrameNot

	UICornerNot.CornerRadius = UDim.new(0.375, 0)
	UICornerNot.Parent = FrameNot

	UIGradientNot.Color = ColorSequence.new{ColorSequenceKeypoint.new(0.00, Color3.fromRGB(170, 0, 255)), ColorSequenceKeypoint.new(1.00, Color3.fromRGB(0, 0, 0))}
	UIGradientNot.Transparency = NumberSequence.new{NumberSequenceKeypoint.new(0.00, 0.00), NumberSequenceKeypoint.new(0.50, 0.26), NumberSequenceKeypoint.new(0.75, 0.48), NumberSequenceKeypoint.new(1.00, 0.69)}
	UIGradientNot.Parent = FrameNot

	TextLabelNot.Parent = FrameNot
	TextLabelNot.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	TextLabelNot.BackgroundTransparency = 1.000
